export SYSTEM_NAME="smitty"
export AWS_REGION=us-east-1
export BASE16_SHELL=$HOME/.config/base16-shell/
export BROWSER=brave
export COMPLETION_WAITING_DOTS="true"
export CURL_CA_BUNDLE=$HOME/.certs/cacert.pem
export EDITOR="$VISUAL"
export GOPATH=$HOME/Code/go
export GOROOT=/usr/local/go
export LANG=en_US.UTF-8
export MANPAGER=$PAGER
export PAGER=less
export SSH_AUTH_SOCK=~/.ssh/ssh_auth_sock
export TERMINAL=iTerm2
export VISUAL=$(which vim)
export YAMLLINT_CONFIG_FILE=$HOME/.yamllint
export ZSH=$HOME/.oh-my-zsh
export ZSH_CUSTOM=$HOME/.zsh_custom
export ZSH_THEME="ssmith.custom"
export VSCODE_EXTENSIONS="$HOME/Library/Application\ Support/Code/User/extensions.json"
